
@model E2BizzEventManagementSystem.Model.Employee

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@ViewBag.PageTitle @Model.EmployeeName</h1>

<table class="table table-hover table-striped">
    <tbody>
        <tr>
            <th>Employee Id</th>
            <td>@Model.EmployeeId</td>
        </tr>
        <tr>
            <th>Employee Name</th>
            <td>@Model.EmployeeName</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>City</th>
            <td>@Model.City</td>
        </tr>
        <tr>
            <th>Zipcode</th>
            <td>@Model.Zipcode</td>
        </tr>
        <tr>
            <th>Country</th>
            <td>@Model.Country</td>
        </tr>
        <tr>
            <th>Contact #</th>
            <td>@Model.Phone</td>
        </tr>
        <tr>
            <th>Email Id</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Skillsets</th>
            <td>@Model.Skillsets</td>
        </tr>
        <tr>
            <th>Avatar</th>
            <td>
                <img src="@Url.Content(Model.Avatar)" alt="@Model.EmployeeName" />
            </td>
        </tr>
    </tbody>
</table>

